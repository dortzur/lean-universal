{
  "name": "lean-universal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js",
    "build": "rimraf dist && rimraf build && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress --profile --colors",
    "start": "node server"
  },
  "babel": {
    "presets": [
      [
        "es2015"
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.4",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "html-webpack-plugin": "^2.26.0",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.0-rc.4",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "babel-register": "^6.18.0",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.9.8",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-transmit": "^3.2.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "~2.3.0",
    "stats-webpack-plugin": "^0.4.3",
    "webpack-node-externals": "^1.5.4"
  }
}
