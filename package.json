{
  "name": "lean-universal",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js",
    "build": "rimraf dist && rimraf build && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress --profile --colors",
    "start": "node server",
    "cold-start": "npm run build && node server"
  },
  "babel": {
    "presets": [
      [
        "es2015"
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "transform-decorators-legacy"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "ignore-loader": "^0.1.2",
    "ignore-styles": "^5.0.1",
    "postcss-loader": "^1.2.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.4",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "dependencies": {
    "async-props-promise": "^0.4.3",
    "autoprefixer": "latest",
    "babel-polyfill": "^6.20.0",
    "babel-register": "^6.18.0",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "~1.8.0",
    "node-sass": "^4.3.0",
    "node-sass-middleware": "0.11.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.0",
    "react-transmit": "^3.2.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^5.0.1",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "~2.3.0",
    "stats-webpack-plugin": "^0.4.3",
    "webpack-node-externals": "^1.5.4"
  }
}
